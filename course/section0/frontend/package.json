{
  "name": "vue3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "npx pretty-quick"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "js-cookie": "^3.0.5",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.19.30",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "~5.3.3",
    "vite": "^6.2.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  }
}
